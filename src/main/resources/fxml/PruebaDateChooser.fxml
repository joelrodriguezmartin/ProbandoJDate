<?xml version="1.0" encoding="UTF-8"?>

<?import dad.javafx.componentes.DateChooser?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="root" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="initializeButton" maxWidth="75.0" mnemonicParsing="false" text="Inicializar" HBox.hgrow="ALWAYS" />
            <Button fx:id="consultButton" maxWidth="75.0" mnemonicParsing="false" text="Consultar" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </center>
   <top>
      <DateChooser fx:id="dateChooser" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
